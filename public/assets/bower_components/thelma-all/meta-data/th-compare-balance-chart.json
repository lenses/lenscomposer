{
  "name": "th-compare-balance-chart",
  "description": "Thelma Compare Balance",
  "category":"textual",
  "version": "0.0.1",
  "thumbnail":"balance.jpg", 
  "tags":["chart", "image"],
  "helpLink":"",
  "width":200,
  "height":250,
  "inputAttr": {
    "chartWidth":{"friendly":"Width", "type":"string", "default":"100%"},
    "chartHeight":{"friendly":"Height", "type":"string", "default":"100%"},
    "chartData":{"friendly":"Data", "type":"table-fixed","rows":2, 
      "fields":{
        "label":{"friendly":"Label", "type":"string", "default":"Smoker"},
        "count":{"friendly":"Count", "type":"int", "default":3},
        "weight":{"friendly":"Weight", "type":"float", "default":1},
        "image":{"friendly":"Image", "type":"image", "default":"/bower_components/thelma-components/images/kid.png"},
        "size":{"friendly":"Size(px)", "type":"int", "default":23}
      },
      "default":[
        {"label": "Smoker", "count": 3, "weight": 1, "image": "/bower_components/thelma-components/images/kid-smoker.png", "size":23},
        {"label": "Non-Smoker", "count": 50, "weight": 1, "image": "/bower_components/thelma-components/images/kid.png", "size":23}
      ]
    }
  }
}