{
  "name": "th-google-map",
  "description": "Wrapper for Google Maps",
  "category":"map",
  "version": "0.0.1",
  "thumbnail":"google-map.jpg", 
  "tags":["map", "google"],
  "helpLink":"",
  "width":280,
  "height":200,
  "inputAttr": {
    "width":{"friendly":"Width", "type":"string", "default":"100%"},
    "height":{"friendly":"Height", "type":"string", "default":"100%"},
    "latitude": {"friendly":"Latitude", "type":"int", "default":40.7127},
    "longitude": {"friendly":"Longitude", "type":"int", "default":-74.0059},
    "zoom": {"friendly":"Zoom", "type":"int", "default":10},
    "useTheme": {"friendly":"Use Card Theme", "type":"boolean", "default": true},
    "showCenterMarker": {"friendly":"Show Center Marker", "type":"boolean", "default": false},
    "fitToMarkers": {"friendly":"Fit to Markers", "type":"boolean", "default": false},
    "zoomable": {"friendly":"Zoomable", "type":"boolean", "default": true},
    "mapType": {"friendly":"Map Type", "type": "dropdown", "default": "roadmap", "values":["roadmap", "satellite", "hybrid", "terrain"]},
    "markers":{"friendly":"Markers", "type":"table-repeating","rows":2, 
      "fields":{
        "latitude": {"friendly":"Latitude", "type":"int", "default":40.7127},
        "longitude": {"friendly":"Longitude", "type":"int", "default":-74.0059},
        "label" : {"friendly": "Label", "type": "string", "defualt": "NYC"},
        "image" : {"friendly": "Icon", "type": "image", "defualt": ""}
      },
      "default":[
        {"latitude": 40.71, "longitude": -74.0059, "label": "NYC"  }, 
        {"latitude": 40.75, "longitude": -74.0049, "label": "another"  }
      ]

    }
  }
}
