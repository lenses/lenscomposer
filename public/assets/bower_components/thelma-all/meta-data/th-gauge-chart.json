{
  "name": "th-gauge-chart",
  "description": "Thelma gauge chart",
  "category":"graph",
  "version": "0.0.1",
  "thumbnail":"gauge.jpg", 
  "tags":["graph", "gauge", "data", "animated"],
  "helpLink":"",
  "width":200,
  "height":125,
  "inputAttr": {
    "chartWidth":{"friendly":"Width", "type":"string", "default":"100%"},
    "chartData":{"friendly":"Data", "type":"table-fixed","rows":1, 
      "fields":{
        "value":{"friendly":"Value", "type":"int", "default":65},
        "min":{"friendly":"min", "type":"int", "default":0},
        "max":{"friendly":"max", "type":"int", "default":100},
        "ticks":{"friendly":"ticks", "type":"int", "default":5}
      },
       "default":[{"value": 65, "min":0, "max":100, "ticks":5}]
    }
  }
}